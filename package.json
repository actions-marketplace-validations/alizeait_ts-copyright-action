{
  "name": "ts-copyright-action",
  "version": "1.0.0",
  "description": "Adds Copyright text to TypeScript and JavaScript files",
  "main": "./dist/index.js",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup ./src/index.ts  --format cjs --legacy-output --clean --minify",
    "test": "jest",
    "check-types": "tsc",
    "start": "ts-node ./src/index.ts"
  },
  "sideEffects": false,
  "homepage": "https://github.com/alizeait/ts-copyright-action#readme",
  "bugs": {
    "url": "https://github.com/alizeait/ts-copyright-action/issues"
  },
  "author": {
    "name": "Ali Zeaiter",
    "email": "rassoali.arz@gmail.com"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alizeait/ts-copyright-action.git"
  },
  "files": [
    "dist/**"
  ],
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "glob": "^9.3.0",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5"
  },
  "packageManager": "yarn@3.5.0"
}
